{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "inputs": [
        {
          "type": "pickString",
          "id": "examples",
          "description": "Which ros pkg do like?",
          "options": [
            "shmfw_demo",
            "shmfw_usage_var",
            "shmfw_usage_vector"
          ],
          "default": "shmfw_usage_vector"
        },
        {
          "type": "pickString",
          "id": "apps",
          "description": "Which ros pkg do like?",
          "options": [
            "shmfw_editor",
            "shmfw_admin"
          ],
          "default": "shmfw_editor"
        },
    ],
    "configurations":  [

        
        {
            "name": "launch example",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/common/examples/${input:examples}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
        {
            "name": "launch app",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/common/apps/${input:apps}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}